{
  "version": 2,
  "buildCommand": "pip install -r requirements.txt",
  "installCommand": "pip install -r requirements.txt",
  "builds": [
    {
      "src": "src/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.py"
    }
  ],
  "functions": {
    "src/main.py": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
